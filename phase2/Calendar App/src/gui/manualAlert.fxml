<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="77.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ManualAlert">
    <children>
        <ButtonBar layoutY="110.0" prefHeight="40.0" prefWidth="200.0">
            <buttons>
                <Button mnemonicParsing="false" text="Done"/>
            </buttons>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </ButtonBar>
        <TextField layoutX="10.0" layoutY="65.0" promptText="24 hour time format" AnchorPane.leftAnchor="10.0"
                   AnchorPane.rightAnchor="10.0"/>
        <DatePicker layoutX="6.0" layoutY="20.0" promptText="Pick a date" AnchorPane.leftAnchor="10.0"
                    AnchorPane.rightAnchor="10.0"/>
    </children>
</AnchorPane>
